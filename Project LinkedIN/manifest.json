{
  "manifest_version": 3,
  "name": "LinkedIn Profile Enhancer Widget",
  "version": "1.0",
  "description": "Injects a widget on LinkedIn profiles",
  "permissions": ["storage", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://www.linkedin.com/in/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_title": "LinkedIn Widget"
  }
}